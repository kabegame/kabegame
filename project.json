{
  "name": "kabegame",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite"
      },
      "dependsOn": ["crawler-plugins:package"]
    },
    "dev-local-plugins": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite"
      },
      "dependsOn": ["crawler-plugins:package-local"]
    },
    "tauri:dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tauri dev"
      },
      "dependsOn": ["crawler-plugins:package"]
    },
    "tauri:dev-local-plugins": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tauri dev"
      },
      "dependsOn": ["crawler-plugins:package-local"]
    },
    "tauri:dev:watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tauri dev --additional-watch-folders ..\\crawler-plugins"
      },
      "dependsOn": ["crawler-plugins:package"]
    },
    "tauri:dev:watch-local-plugins": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tauri dev --additional-watch-folders ..\\crawler-plugins"
      },
      "dependsOn": ["crawler-plugins:package-local"]
    }
  }
}
