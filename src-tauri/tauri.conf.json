{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Kabegame",
  "version": "0.1.0",
  "identifier": "com.kabegame",
  "build": {
    "beforeDevCommand": "vite",
    "beforeBuildCommand": "vue-tsc && vite build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Kabegame",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/icon.icns", "icons/icon.ico"]
  }
}

